{
  "name": "linter-docker",
  "main": "./lib/init",
  "version": "0.3.2",
  "description": "Linter plugin for Dockerfile's, using dockerlint",
  "repository": "https://github.com/AtomLinter/linter-docker.git",
  "private": true,
  "license": "MIT",
  "engines": {
    "atom": ">=1.4.0 <2.0.0"
  },
  "scripts": {
    "test": "apm test",
    "lint": "eslint ."
  },
  "dependencies": {
    "atom-linter": "^10.0.0",
    "atom-package-deps": "^4.6.0",
    "dockerlint": "0.3.9"
  },
  "devDependencies": {
    "eslint": "^5.0.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "jasmine-fix": "^1.3.0"
  },
  "package-deps": [
    "linter:2.0.0"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "global-require": "off",
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "atom"
          ]
        }
      ]
    },
    "globals": {
      "atom": true
    },
    "env": {
      "node": true,
      "browser": true
    }
  }
}
